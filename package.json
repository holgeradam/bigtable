{
  "name": "bigtable",
  "version": "0.0.1",
  "description": "Convenience wrapper around @google-cloud/bigtable",
  "main": "dist/index.js",
  "repository": "git@github.com:knamp/static-styles.git",
  "author": "Christian Froehlingsdorf <info@5cf.de>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigtable": "~0.14.0"
  },
  "devDependencies": {
    "@types/node": "^10.3.6",
    "mocha": "~5.2.0",
    "tslint": "~5.10.0",
    "typescript": "~2.9.2"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc -p tsconfig.json",
    "_start": "DEBUG=bigtable:* ts-node example/index.ts",
    "start": "yarn build && yarn _start",
    "lint": "tslint -c tslint.json '**/*.ts'",
    "test": "yarn build && DEBUG=bigtable:* ./node_modules/.bin/_mocha --recursive --timeout 30000 --exit -R spec test/int"
  }
}
